{
	"server": {
		"public": "https://[This Server's Domain Name: Include Port if Applicable]/",
		"port": [Port Number],
		"web_root": "./ui",
		"-redirect": "https://site.which.to/redirect/any/web/requests",
		"recover_mode": false,
		"api": {
			"log_all_requests": true,
			"log_post_body": false,
			"allow_modifications": false,
			"database_control": false
		},

		"-certificate": "/path/to/cert",
		"-authority": "/path/to/auth_cert",
		"-key": "/path/to/key",
		
		"origins": {
			"https://sistersite.example": [TimeStamp to expire access],
			"https://yoursite.example": 0,
			"http://127.0.0.1:3082": 0,
			"http://127.0.0.1:3000": 0,
			"postman"
		}
	}
}
